/->/ {
	return ASSIGN
}

/AND/ {
	return AND
}

/OR/ {
	return OR
}

/NOT/ {
	return NOT
}

/LSHIFT/ {
	return LSHIFT
}

/RSHIFT/ {
	return RSHIFT
}

/[0-9]+/ {
	value, _ := strconv.Atoi(yylex.Text())
	lval.node = Literal{Value(value)}
	return LITERAL
}

/[a-z][a-z0-9]*/ {
	lval.node = Identifier{yylex.Text()}
	return IDENTIFIER
}

/[ \t\n]+/ {
}

